#include <iostream>
#include <cmath>
using namespace std;
int main() {
	cout << "Min large number from 20: \n";
	const int N = 30;
	int arr[N];
	for (int i = 0; i < N; i++) arr[i] = rand() % 101;
	int min = 100;
	for (int i = 0; i < N; i++)
	{
		if (arr[i] > 19) if (arr[i] < min) min = arr[i];
	}
	cout << min;
        return 0;
}
